<% provide(:title, "Войти") %>
<div class="img-edit">
  <%= link_to image_tag("moya_vesna.png", alt: "Весна"), root_path %>
</div>  

<div class="row">
  <div class="span6 offset3">
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %><br />

      <%= f.label :password, "Пароль" %>
      <%= f.password_field :password %><br />

      <%= f.submit "Войти", class: "btn btn-large btn-primary" %>
    <% end %>
     <div class="reg">  
       Новый пользователь? <%= link_to "Зарегистрироваться!", signup_path %>
     </div>  
  </div>
</div>